{
  "name": "konekod/evenly-distribute",
  "description": "Universal service for even distribution of anything",
  "version": "1.0.0",
  "type": "library",
  "keywords": [
    "evenly",
    "distribute",
    "sort",
    "group"
  ],
  "require": {
    "php": ">=8.3"
  },
  "scripts": {
    "phpstan": "vendor/bin/phpstan analyse --level=max src tests"
  },
  "license": "LGPL-3.0-only",
  "autoload": {
    "psr-4": {
      "KoNekoD\\EvenlyDistribute\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "KoNekoD"
    }
  ],
  "minimum-stability": "stable",
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "spatie/phpunit-snapshot-assertions": "^4.2",
    "phpstan/phpstan": "^1.11"
  }
}
